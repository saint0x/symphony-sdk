{
  "name": "symphonic",
  "version": "0.3.6",
  "description": "Symphony SDK - An advanced multi-agent orchestration framework",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./types/*": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/types/*.js",
      "require": "./dist/types/*.cjs"
    },
    "./symphony/*": {
      "types": "./dist/symphony/*.d.ts",
      "import": "./dist/symphony/*.js",
      "require": "./dist/symphony/*.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -b",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "docs": "typedoc src/index.ts",
    "clean": "rimraf dist",
    "prepare": "bun run build",
    "prepack": "bun run build"
  },
  "keywords": [
    "ai",
    "sdk",
    "tools",
    "agents",
    "teams",
    "pipelines",
    "typescript",
    "directives",
    "chaining"
  ],
  "author": "Saint",
  "license": "MIT",
  "dependencies": {
    "dotenv": "^16.3.1",
    "openai": "^4.71.0",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "uuid": "^9.0.0",
    "winston": "^3.10.0",
    "xml2js": "^0.6.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^22.10.2",
    "@types/uuid": "^9.0.2",
    "@types/xml2js": "^0.4.14",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "jest": "^29.6.2",
    "prettier": "^3.0.1",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^8.3.5",
    "typedoc": "^0.24.8",
    "typescript": "^5.7.2"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0"
  }
}
