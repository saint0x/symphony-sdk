{
  "name": "symphonic",
  "version": "0.1.0",
  "description": "A TypeScript SDK for building AI-powered applications with tools, agents, teams and pipelines",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./tools": {
      "require": "./dist/tools/index.js",
      "import": "./dist/tools/index.mjs",
      "types": "./dist/tools/index.d.ts"
    },
    "./agents": {
      "require": "./dist/agents/index.js",
      "import": "./dist/agents/index.mjs", 
      "types": "./dist/agents/index.d.ts"
    },
    "./teams": {
      "require": "./dist/teams/index.js",
      "import": "./dist/teams/index.mjs",
      "types": "./dist/teams/index.d.ts"
    },
    "./pipelines": {
      "require": "./dist/pipelines/index.js",
      "import": "./dist/pipelines/index.mjs",
      "types": "./dist/pipelines/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "docs": "typedoc src/index.ts",
    "clean": "rimraf dist",
    "prepare": "npm run build",
    "prepack": "npm run build"
  },
  "keywords": [
    "ai",
    "sdk",
    "tools",
    "agents",
    "teams",
    "pipelines",
    "typescript"
  ],
  "author": "Saint Asky",
  "license": "MIT",
  "dependencies": {
    "@grpc/grpc-js": "^1.9.0",
    "@grpc/proto-loader": "^0.7.8",
    "openai": "^4.0.0",
    "winston": "^3.10.0",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.3",
    "@types/node": "^20.4.8",
    "@types/uuid": "^9.0.2",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "jest": "^29.6.2",
    "prettier": "^3.0.1",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.0",
    "typedoc": "^0.24.8",
    "typescript": "^5.1.6"
  },
  "sideEffects": false,
  "engines": {
    "node": ">=18.0.0"
  }
}
